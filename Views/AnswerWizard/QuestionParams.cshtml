<!-- Modal for Search History-->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Add to your search history</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label class="col-form-label">Search Name:</label>
                        <input type="text" id="searchNameInput" class="form-control">
                    </div>

                </form>
            </div>
            <div class="modal-footer">
                @* <button type="button" class="btn waves-effect waves-light btn-custom serchcss B_s003" data-dismiss="modal">Close</button>*@
                <button type="button" id="saveSearchButton" class="btn waves-effect waves-light btn-custom serchcss B_s002 Searchbtn"
                        onclick="saveSearch()">
                    Save
                </button>
            </div>
        </div>
    </div>
</div>


<!-- Modal for Socioeconomic-->
<div class="modal fade" id="Mysocio" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg modal-dialog-scrollable" style="width: 45%;">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">

                <h4 class="modal-title" id="socioHeading">Choose Socioeconomic set-aside</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div id="SBusiness" class="">
                    </div>
                    <div style="width: 100%;padding: 0px 19px;height: 600px;">
                        <div class="table-responsive">
                            <table id="tbl_Socio" class="table table-striped table-bordered" style="width:100% !important">
                                <thead>
                                    <tr id="HeadR">
                                        <th><label>Row #</label></th>
                                        <th style="width:50%;"><label>Description</label></th>
                                        <th><label>Abbr.</label></th>
                                        <th><label>Select</label></th>
                                    </tr>
                                </thead>
                                <tbody class="clsSocioeconomic tbodyContent" id="tbody_Socio"></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default okaySocio" id="OKSocio">OK</button>
                <button type="button" class="btn btn-secondary cancelSocio" id="CancelSocio">Cancel</button>
            </div>
        </div>

    </div>
</div>

