<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
<link rel="stylesheet" type="text/css" href="~/Content/AppCss/answerWizard.css" />
<link rel="stylesheet" type="text/css" href="~/Content/assets/libs/bootstrap-multiselect/bootstrap-multiselect.css" />

<script src="https://code.jquery.com/ui/1.13.2/jquery-ui.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<style>
    .nav-item {
        cursor: pointer;
    }

    .heading {
        color: purple;
    }

    .breadcrum {
        color: purple;
    }

    .pull-right {
        float: left !important;
    }

    .btn-purple {
        background-color: purple;
        color: white;
    }

    .wrapper, .attachments-show {
        display: none;
        padding: 20px;
        margin: 0px 0;
    }

    textarea {
        border: 1px solid #DDDFE2;
        width: 100%;
        color: #6e6e6e;
        font-family: 'Rubik';
        text-decoration: none;
        display: inline;
        font-weight: 400;
        font-size: 13px;
        background: #fff;
        border-radius: 10px;
        padding: 15px;
    }

    .comnt-load {
        margin: 0px;
        padding: 0px;
        color: #6e6e6e;
        text-decoration: none;
        display: flex;
        font-weight: 500;
        font-size: 14px;
        margin-left: 8px;
    }

        .comnt-load:focus {
            box-shadow: none;
        }

    .attachment-load {
        margin: 0px;
        padding: 0px;
        color: #6e6e6e;
        text-decoration: none;
        display: flex;
        font-weight: 500;
        font-size: 14px;
        margin-left: 8px;
    }

    button.attachment-load:focus {
        box-shadow: none;
    }

    button.comnt {
        color: #fff;
        background-color: #571C7A;
        border-color: #571C7A;
        font-family: 'Rubik';
        font-weight: 400;
        padding: 6px 20px 6px 20px;
        font-size: 14px;
    }

        button.comnt:hover {
            color: #fff;
            background-color: #218838;
            border-color: #1e7e34;
        }

    .comment__list {
        background-color: #DDDFE2;
        border-radius: 3px;
    }

    .companyblock .likeblock .likebtn {
        visibility: hidden;
    }
</style>



<div class="container" style="margin-left:0px !important;">
    <div class="row">
        <div class="col-12">
            <div class="scp-breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item active">Wizard</li>
                </ol>
            </div>
        </div>
    </div>
    <div class="row">
        <p class="breadcrumb-item" style=" padding-left: 15px;"><a href="https://www.youtube.com/playlist?list=PLLqOry17kGuSRV04rPnyhq15l6Wd2nLPv" target="_blank">Watch these short videos to get started</a></p>
    </div>
</div>


<div class="container-fluid" style="margin-left:0px !important;">
    <div class="row">
        <div class="col-12">
            <div class="wizards_con">
                <div class="first_row">
                    <div id="agency" onclick="routeTo('agency')">
                        <span>Agency</span>
                    </div>
                    <div id="contract"  onclick="routeTo('contract')">
                        <span>Contract</span>
                    </div>
                </div>
                <div class="second_row">
                    <div id="vendor"  onclick="routeTo('vendor')">
                        <span>Vendor</span>
                    </div>
                    <div id="opport"  onclick="routeTo('opportunity')">
                        <span>Opportunity</span>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>





<script src="~/Scripts/AppJS/AnswerWizard.js"></script>
