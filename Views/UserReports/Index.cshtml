
@{
    ViewBag.Title = "User Reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .container {
        display: block;
        /*margin: 17px 10px;*/
        /* padding: 11px; */
        position: relative;
        padding-left: 27px;
        font-family: Rubik,sans-serif;
        /* margin-bottom: 12px; */
        cursor: pointer;
        font-weight: 300;
        font-size: 13px;
        -webkit-user-select: none;
        -moz-user-select: none;
        /* width: auto; */
        float: right;
        -ms-user-select: none;
        user-select: none;
        text-align: left;
    }
    .excel-btn{
        width: 35px;
    }
</style>

<link href="~/Content/AppCss/Search.css" rel="stylesheet" />
<link href="~/Content/AppCss/toggle.css" rel="stylesheet" />
<script src="~/Scripts/AppJS/UserReport.js"></script>

<div class="innerbody">
    <div class="col-lg-12 row">
        <div class="col-lg-3" style="padding: 0;flex: 0 0 20%;">
            <nav class="breadcrumb" style="padding: 0px 2px;margin-bottom: 10px;">
                <a class="breadcrumb-item" id="sear"> User Reports</a>
            </nav>
        </div>
    </div>
    <div class="row">
        <div class="card card-body">
            <div class="row s027">
                <div class="col-md-5 offset-md-3 btn-group btn-toggle mb-2 s003">
                    <button class="btn s011 Com_006 TypeOne" id="Type1" data-toggle="collapse" data-target="#collapsible"> User Report</button>
                    <button class="btn active s011 Com_005 TypeTwo" id="Type2" data-toggle="collapse" data-target="#collapsible"> Global Report</button>
                    <button class="btn active s011 Com_005 TypeThree" id="Type3" data-toggle="collapse" data-target="#collapsible"> Opportunity Daily Email</button>
                </div>

                <div class="row col-md-12" id="contract" style="margin-top: 12px;">

                    <div class="col-md-12">
                        <div class="gridTbl" style="width: 100%;margin-left: 0px;border: 1px solid #e6e6e6;padding: 10px;">
                            <table id="UserReportGrid_tbl" class="table table-striped table-bordered dt-responsive dataTable" style="font-size: 11px; font-family: sans-serif; width: 100%;">
                                <thead>
                                    <tr>
                                        <th title="Row"><label>Report#</label></th>
                                        <th title="Name"> <label>Report Name</label> </th>
                                        <th title="DateTime"> <label> Date Created</label> </th>
                                        <th title="Note"> <label> Details</label> </th>
                                        <th title="Download"> <label> Download Report</label> </th>

                                    </tr>
                                </thead>
                                <tbody id="UserReportGrid_tbody" style="font-size:15px !important;"> </tbody>
                            </table>
                        </div>

                    </div>
                </div>

                <div class="row col-md-12" id="opportunity" style="margin-top: 12px; display:none">

                    <div class="col-md-12">
                        <div class="gridTbl" style="width: 100%;margin-left: 0px;border: 1px solid #e6e6e6;padding: 10px;">
                            <table id="OpportunityGrid_tbl" class="table table-striped table-bordered dt-responsive dataTable" style="font-size: 11px; font-family: sans-serif; width: 100%;">
                                <thead>
                                    <tr>
                                        <th title="Row"><label>Report#</label></th>
                                        <th title="Daily Report Name"> <label>Daily Report Name</label> </th>
                                        <th title="NAICS Code"> <label>NAICS Code</label> </th>
                                        <th title="Funding Agency"> <label>Funding Agency</label> </th>
                                        <th title="Funding sub Agency"> <label>Funding sub Agency</label> </th>
                                        <th title="Keyword"> <label>Keyword</label> </th>
                                        <th title="Socio Economic Designation"> <label>Socio Economic Designation</label> </th>
                                        <th title="Base Type"> <label>Base Type</label> </th>
                                        <th title="Action"> <label>Action</label> </th>

                                    </tr>
                                </thead>
                                <tbody id="OpportunityGrid_tbody"> </tbody>
                            </table>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
