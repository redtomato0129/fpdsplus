
@{
    ViewBag.Title = "Unauthorized Access";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .img-accessdenied img{
        margin: 0 auto;
        display: block; 
    }
    .img-accessdenied a {
        background: #f1f1f1;
        font-size: 20px;
        padding: 5px 15px;
        border-radius: 10px;
        font-weight: 500;
    }
</style>

<div class="innerbody">
    <div class="col-lg-12 row">
        <div class="col-lg-3" style="padding: 0;flex: 0 0 20%;">
            <nav class="breadcrumb" style="padding: 0px 2px;margin-bottom: 10px;">
                <a class="breadcrumb-item" id="sear">Access Denied</a>
            </nav>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body text-center">
                <div class="img-accessdenied">
                    <img src="~/Content/assets/images/AccessDenied.svg" alt="" />
                    <h4>You are not allowed to access this content.</h4>
                    <a href="~/Payment/PaymentWithPaypal" alt="FedPipeLine Subscription Link">Click here to buy new subscription plan</a>
                </div>
                </div>
            </div>
        </div>
    </div>
</div>

