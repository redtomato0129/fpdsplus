<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<div class="container" style="margin-left:0px !important;">
    <div class="row">
        <div class="col-12">
            <div class="scp-breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item" id="page1"><a href="/AnswerWizard/Index">Wizard</a></li>
                    <li class="breadcrumb-item active">Help Page</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<div id="userDomain" style="display:none;">
    @Session["User_Domain"].ToString()
</div>
<div class="col-12" id="noData">
    <h1 class="no_data">Unauthorized Access</h1>
</div>
<div class="container-fluid" style="margin-left:0px !important;" id="helpPageContent">
    <div class="row p-0">
        <div class="col-md-12">
            <label>Select Question ID</label>
            <select style="width:80px" id="questionId" onchange="questionIdChange(this);">            
            </select>
        </div>
       
        <label>Search</label>
        <textarea id="search_text"></textarea>
        <label>Dashboard</label>
        <textarea id="dashboard_text"></textarea>
        <label>Drilldown</label>
        <textarea id="drilldown_text"></textarea>
        <label>Data Grid</label>
        <textarea id="datagrid_text"></textarea>
        <button onclick="saveHelpData()">Save</button>
    </div>
    
</div>

