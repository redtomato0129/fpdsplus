    /*Css by Usama*/
    div.popup_content {
        display: none;
        position: absolute;
        width: 180px;
        padding: 10px;
        background: #eeeeee;
        color: #000000;
        border: 1px solid #1a1a1a;
        font-size: 16px;
        font-weight: 500;
    }

    .popup_content ul {
        list-style: none;
        padding: 0;
        margin: 0;
    }


        .popup_content ul li {
            display: flex;
            align-items: center;
        }

            .popup_content ul li div {
                width: 12px;
                height: 12px;
                background-color: transparent;
            }

            .popup_content ul li:first-child div {
                background-color: red;
            }

            .popup_content ul li:nth-child(1) div {
                background-color: #4040a3;
            }

            .popup_content ul li:nth-child(2) div {
                background-color: #c74747;
            }

            .popup_content ul li:nth-child(3) div {
                background-color: blueviolet;
            }

            .popup_content ul li:nth-child(4) div {
                background-color: orange;
            }

            .popup_content ul li:nth-child(5) div {
                background-color: magenta;
            }

            .popup_content ul li:nth-child(6) div {
                background-color: blueviolet;
            }

    .dropzone {
        background: white;
        border-radius: 5px;
        border: 2px dashed rgb(0, 135, 247);
        border-image: none;
        max-width: 500px;
        margin-left: auto;
        margin-right: auto;
