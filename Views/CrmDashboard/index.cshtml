@{
    ViewBag.Title = "Dashboard";

    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
@*<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>*@
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>

<script src="https://code.jquery.com/ui/1.13.2/jquery-ui.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css" integrity="sha512-xh6O/CkQoPOWDdYTDqeRdPCVd1SpvCA9XXcUnZS2FmJNp1coAFzvtCN9BmamE+4aHK8yyUHUSCcJHgXloTyT2A==" crossorigin="anonymous" referrerpolicy="no-referrer" />
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<link rel="stylesheet" type="text/css" href="~/Content/AppCss/crmDashboard.css" />
<div id="userId" style="display:none;">
    @Session["User_ID"].ToString()
</div>

<div id="firstName" style="display:none;">
    @Session["User_FirstName"].ToString()
</div>


<div id="email" style="display:none;">
    @Session["User_Email"].ToString()
</div>



<div class="container-fluid dashboard__data">
    <div class="row">
        <div class="col-11 mb-3">
            <h1 class="heading-title"><strong>Dashboard - Latest Updates</strong></h1>
            <h6 class="heading-title">(Click Row for further details)</h6>
        </div>
        <div class="col-1" id="shareIcon" style="cursor: pointer;">
            <img src="~/Content/assets/images/share.png" data-toggle="tooltip" data-placement="bottom" title="Share URL" onclick="copyToClipboard()" style="float: right; width: 30px; height: 30px;" />
        </div>
        <div class="col-12 col-md-6">
            <div class="deals__details">
                <h4><strong>Deals</strong></h4>
                <table class="table table-bordered mt-3">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">Title</th>
                            <th scope="col">Status</th>
                            <th scope="col">Priority</th>
                            <th scope="col">Stage</th>
                            <th scope="col">RFP Release Date</th>
                        </tr>
                    </thead>
                    <tbody id="dealsList">
                    </tbody>
                </table>

            </div>
        </div>
        <div class="col-12 col-md-6">
            <div class="activities__details">
                <h4><strong>Activities</strong></h4>
                <table class="table table-bordered mt-3">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">Title</th>
                            @* <th scope="col">Created Date</th>*@
                            <th scope="col">Activity</th>
                            <th scope="col">Description</th>
                            @* <th scope="col">Type</th>*@
                        </tr>
                    </thead>
                    <tbody id="activityList">
                    </tbody>
                </table>

            </div>
        </div>
        <div class="col-12 col-md-6">
            <div class="peoples__details">
                <h4><strong>People</strong></h4>
                <table class="table table-bordered mt-3">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">Organization</th>
                            <th scope="col">Phone</th>
                            <th scope="col">Email</th>
                            @* <th scope="col">City</th>
                            <th scope="col">State</th>*@
                        </tr>
                    </thead>
                    <tbody id="organizationList">
                    </tbody>
                </table>

            </div>
        </div>
        <div class="col-12 col-md-6">
            <div class="news__details">
                <h4><strong> Social</strong></h4>
                <div class="news__list" id="newsList">

                </div>

            </div>
        </div>


    </div>
</div>




<script src="~/Scripts/AppJS/CrmDashboard.js"></script>