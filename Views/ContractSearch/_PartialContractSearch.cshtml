        }

    .container {
        display: block;
        /*margin: 17px 10px;*/
        /* padding: 11px; */
        position: relative;
        padding-left: 27px;
        font-family: Rubik,sans-serif;
        /* margin-bottom: 12px; */
        cursor: pointer;
        font-weight: 300;
        font-size: 13px;
        -webkit-user-select: none;
        -moz-user-select: none;
        /* width: auto; */
        float: right;
        -ms-user-select: none;
        user-select: none;
        text-align: left;
    }

        /* Hide the browser's default checkbox */
        .container input {
            position: absolute;
            opacity: 0;
            cursor: pointer;
            height: 0;
            width: 0;
        }

    /* Create a custom checkbox */
    .checkmark0 {
        position: absolute !important;
        top: 0 !important;
        left: 60px !important;
        height: 17px !important;
        width: 17px !important;
        background-color: #ffffff !important;
        border-radius: 0%;
        border: 1px solid #d4d4d4;
    }

    /* On mouse-over, add a grey background color */
    .container:hover input ~ .checkmark0 {
        background-color: #ccc;
    }

    /* When the checkbox is checked, add a blue background */
    .container input:checked ~ .checkmark0 {
        background-color: #30bf29 !important;
    }

    /* Create the checkmark/indicator (hidden when not checked) */
    .checkmark0:after {
        content: "" !important;
        position: absolute !important;
        background-color: #30bf29 !important;
        display: none;
    }

    /* Show the checkmark when checked */
    .container input:checked ~ .checkmark0:after {
        display: block;
    }

    /* Style the checkmark/indicator */
    .container .checkmark0:after {
        background: none;
        left: 5px;
        top: 1px;
        border-radius: 0px;
        background-color: #30bf29 !important;
        width: 5px;
        height: 10px;
        border: solid white;
        border-width: 0 3px 3px 0;
        -webkit-transform: rotate(45deg);
        -ms-transform: rotate(45deg);
        transform: rotate(45deg);
    }

    #tbl_Socio_length {
        display: none !important;
    }

    .table-responsive {
        height: inherit;
    }

    #myModal .modal-dialog {
        max-width: 1040px !important;
    }

    #awardingAgencyModal .modal-dialog {
        max-width: 1040px !important;
    }

    #ShowContactSearch .dataTables_wrapper .dt-buttons .dt-button {
        padding: 17px 40px;
        line-height: 1.5;
