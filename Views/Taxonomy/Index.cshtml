<div class="container-fluid" style="margin-left:0px !important;">
    <div class="row">
        <div class="col-12">
			<div class="row">
				<div class="col-lg-8 center">
					<div class="input-group mb-3">
					<label for="basic-url">Department Name</label>
						<div class="input-group-prepend pl-3">
							<span class="input-group-text OpenDeptPopup btndeptrowclear" id="btndeptrowclear_2"
								  data-toggle="tooltip" title="Click to clear the data in this row"><i class="ti-close"></i></span>
						</div>
						<input type="text" class="DIS_002 form-control txtdept" id="txtdept_2"  />
						<label id="lbldept_2" style="display:none;" class="lbldept lblclr"></label>
					</div>
				</div>
			</div>
        </div>
    </div>

	<!-- Search -->
	<div class="row">
		<div class="col-6 form-group has-search pb-2">
			<input type="text" class="form-control" id="searchAgencyInput" onkeyup="searchAgency(event)" placeholder="Search within Agencies">
			<h5 class="records_text" id="agency_records"></h5>
		</div>
		@*<div class="col-6 form-group has-search pb-2">
			<h5 id="agency_records"></h5>
		</div>*@

		<div class="col-6 form-group has-search pb-2">
			<input type="text" class="form-control" id="searchOfficeInput" onkeyup="searchOffice(event)" placeholder="Search within Offices">
			<h5 class="records_text" id="office_records"></h5>
		</div>
		@*<div class="col-6 form-group has-search pb-2">
			<h5 id="office_records"></h5>
		</div>*@
	</div>

	<div class="row">
		
		<div class="col-6 agencyTable">
			<table class="table table-striped" id="searchAgencyTable">
				<thead class="bg-purple-header text-white">
					<tr>
						<th>#</th>
						<th>Agency Name</th>
					</tr>
				</thead>
				<tbody id="tableBodyAgency">
				</tbody>
			</table>
		</div>

		<div class="col-6 officeTable">
			<table class="table table-striped" id="searchOfficeTable">
				<thead class="bg-purple-header text-white">
					<tr>
						<th>#</th>
						<th>Office Name</th>
					</tr>
				</thead>
				<tbody id="tableBodyOffice">
				</tbody>
			</table>
		</div>

	</div>
</div>





