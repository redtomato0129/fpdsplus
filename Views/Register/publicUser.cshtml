@{
    ViewBag.Title = "Public User";
    Layout = "~/Views/Shared/_LoginPartial.cshtml";
}
<link href="~/Content/AppCss/publicUser.css" rel="stylesheet" />
<script src="~/Scripts/AppJS/publicUser.js"></script>
<style>
    .card {
        min-height: 250px;
    }

    .plan-name {
        font-weight: 500;
    }

    .btn-subsciber {
        background: #ffc439;
        display: block;
        width: 100%;
        padding: 8px 12px;
    }

        .btn-subsciber img {
            width: 72px;
        }

        .btn-subsciber span {
            font-size: 16px;
            color: #525252;
            padding-left: 4px;
        }

    .subscribe-plan-sec {
        background: rgb(42 189 36 / 6%);
        padding: 55px 35px 55px 35px;
        box-shadow: 0 0 6px #dddcdc;
        cursor: pointer;
    }

    .plan-name {
        padding: 55px 35px 32px 35px;
        text-align: center;
    }

        .plan-name h2 {
            color: #571C7A;
            font-weight: 500;
            border-bottom: 2px solid #2abd24;
            display: inline-block;
            padding-bottom: 6px;
            font-size: 26px;
        }

    .user-limit {
        margin-bottom: 0;
        font-size: 20px;
        color: #2abd24;
        font-weight: 400;
        padding-bottom: 12px;
    }
    

    .price-user-package-sec h3 {
        margin-bottom: 0;
        color: #999;
        font-size: 20px;
        font-weight: 400;
        padding-bottom: 30px;
    }

    .price-user-package-sec h1 {
        margin-bottom: 0;
        color: #000;
        font-size: 34px;
        font-weight: 500;
        padding-bottom: 9px;
    }

    .subscribe-description {
        margin-bottom: 0;
        font-size: 16px;
        color: #666;
        line-height: 25px;
        padding-bottom: 26px;
        text-align: center;
    }

    .btn-purchase {
        background: #571C7A;
        color: #fff;
        border-radius: 7px;
        padding: 8px 30px;
        font-size: 16px;
        border: 1px solid #571C7A;
        display: block;
        margin: 0 auto;
    }

        .btn-purchase:hover {
            background: transparent;
            color: #571C7A;
        }

    .subscribe-plan-sec:hover {
        background: linear-gradient(355deg, rgb(242 221 255), #ffffff);
    }

    .auth-wrapper.register {
        display: block !important;
        margin: 0 auto;
    }

        .auth-wrapper.register .auth-box {
            margin: 0 auto !important;
        }

    .plan-sec {
        padding: 65px 355px;
    }

    .price-user-package-sec {
        text-align: center;
    }
</style>
